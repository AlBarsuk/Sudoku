<Window x:Class="MySudoku.BestScores"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MySudoku"
        mc:Ignorable="d"
        Title="BestScores" Height="300" Width="300">
    <Grid>
        <Label x:Name="label" Content="Топ-10 лучших результатов:" HorizontalAlignment="Left" Height="30" Margin="10,27,0,0" VerticalAlignment="Top" Width="249" Background="AliceBlue"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="119" Margin="10,76,0,0" VerticalAlignment="Top" Width="272"/>
        <Button x:Name="button" Content="Вернуться в главное меню" HorizontalAlignment="Left" Height="47" Margin="29,212,0,0" VerticalAlignment="Top" Width="241" Click="button_Click"/>
    </Grid>
</Window>
